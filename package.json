        "@types/loadable__server": "^5.12.5",
        "@types/mocha": "^2.2.40",
        "@types/node": "^7.0.12",
        "@types/react": "^17.0.4",
        "@types/react-dom": "^17.0.3",
        "@types/supertest": "^2.0.0",
        "autod": "^3.0.1",
        "autod-egg": "^1.1.0",
