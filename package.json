{
    "name": "egg-demo",
    "version": "1.0.0",
    "description": "egg demo",
    "private": true,
    "egg": {
        "typescript": true,
        "declarations": true
    },
    "scripts": {
        "start": "egg-scripts start --daemon --title=egg-server-egg-demo",
        "stop": "egg-scripts stop --title=egg-server-egg-demo",
        "dev": "concurrently 'yarn dev:client' 'yarn dev:server'",
        "debug": "egg-bin debug",
        "test-local": "egg-bin test",
        "test": "npm run lint -- --fix && npm run test-local",
        "cov": "egg-bin cov",
        "tsc": "ets && tsc -p tsconfig.json",
        "ci": "npm run lint && npm run cov && npm run tsc",
        "autod": "autod",
        "lint": "eslint . --ext .ts",
        "clean": "ets clean",
        "dev:server": "egg-bin dev",
        "dev:client": "node ./scripts/start.js"
    },
    "dependencies": {
        "@babel/plugin-transform-runtime": "^7.14.5",
        "@babel/runtime": "^7.14.6",
        "@babel/runtime-corejs2": "^7.14.6",
        "@loadable/component": "^5.15.0",
        "@loadable/server": "^5.15.0",
        "egg": "^2.6.1",
        "egg-scripts": "^2.6.0",
        "egg-view-ejs": "^2.0.1",
        "react": "^17.0.2",
        "react-dom": "^17.0.2"
    },
    "devDependencies": {
        "@babel/core": "7.12.3",
        "@babel/plugin-syntax-dynamic-import": "^7.8.3",
        "@babel/preset-env": "^7.14.7",
