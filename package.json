    },
    "repository": {
        "type": "git",
        "url": ""
    },
    "eslintIgnore": [
        "coverage"
    ],
    "author": "liujc",
    "license": "MIT",
    "prettier": {
        "printWidth": 80,
        "tabWidth": 4,
        "useTabs": false,
        "semi": true,
        "singleQuote": true,
        "jsxSingleQuote": false,
